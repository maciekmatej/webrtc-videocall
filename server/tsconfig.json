{
  "extends": "@tsconfig/node22/tsconfig.json",
  "include": ["src/**/*", "src/*", "src/cosrc/server.tserver.ts"],
  "files": [
        "src/server.ts"
    ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "esModuleInterop": true,
    "allowJs": true,
    "incremental": true,
    "outDir": "dist",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node"],
    "lib": ["ESNext","DOM"],
    "skipLibCheck": true,
  }
}